    .port_info 0 /INPUT 1 "sel_left_or_right_rotate";
    .port_info 1 /INPUT 32 "inputData";
    .port_info 2 /INPUT 5 "shiftVal";
    .port_info 3 /OUTPUT 32 "outputData";
