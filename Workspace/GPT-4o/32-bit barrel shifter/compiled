v000001686ce32bb0_0 .net "sel_left_or_right_rotate", 0 0, v000001686cdaba90_0;  1 drivers
v000001686cdab8b0_0 .net "shiftVal", 4 0, v000001686cdabb30_0;  1 drivers
E_000001686ce68530 .event anyedge, v000001686ce32bb0_0, v000001686ce32f60_0, v000001686cdab8b0_0;
    .scope S_000001686cdac3e0;
T_0 ;
    %wait E_000001686ce68530;
    %load/vec4 v000001686ce32bb0_0;
    %cmpi/e 0, 0, 1;
    %jmp/0xz  T_0.0, 4;
    %load/vec4 v000001686ce32f60_0;
    %ix/getv 4, v000001686cdab8b0_0;
    %shiftl 4;
    %load/vec4 v000001686ce32f60_0;
    %pushi/vec4 32, 0, 32;
    %load/vec4 v000001686cdab8b0_0;
    %pad/u 32;
    %sub;
    %ix/vec4 4;
    %shiftr 4;
    %or;
    %store/vec4 v000001686ce32950_0, 0, 32;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v000001686ce32f60_0;
    %ix/getv 4, v000001686cdab8b0_0;
    %shiftr 4;
    %load/vec4 v000001686ce32f60_0;
    %pushi/vec4 32, 0, 32;
    %load/vec4 v000001686cdab8b0_0;
    %pad/u 32;
    %sub;
    %ix/vec4 4;
    %shiftl 4;
    %or;
    %store/vec4 v000001686ce32950_0, 0, 32;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_000001686cdac250;
T_1 ;
    %vpi_call 2 22 "$monitor", "Time=%0t sel=%b input=%h shift=%d output=%h", $time, v000001686cdaba90_0, v000001686cdab950_0, v000001686cdabb30_0, v000001686cdab9f0_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v000001686cdaba90_0, 0, 1;
    %pushi/vec4 2779096485, 0, 32;
    %store/vec4 v000001686cdab950_0, 0, 32;
    %pushi/vec4 1, 0, 5;
    %store/vec4 v000001686cdabb30_0, 0, 5;
    %delay 10000, 0;
    %pushi/vec4 8, 0, 5;
    %store/vec4 v000001686cdabb30_0, 0, 5;
    %delay 10000, 0;
    %pushi/vec4 16, 0, 5;
    %store/vec4 v000001686cdabb30_0, 0, 5;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v000001686cdaba90_0, 0, 1;
    %pushi/vec4 1, 0, 5;
    %store/vec4 v000001686cdabb30_0, 0, 5;
    %delay 10000, 0;
    %pushi/vec4 8, 0, 5;
    %store/vec4 v000001686cdabb30_0, 0, 5;
    %delay 10000, 0;
    %pushi/vec4 16, 0, 5;
    %store/vec4 v000001686cdabb30_0, 0, 5;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 5;
    %store/vec4 v000001686cdabb30_0, 0, 5;
